<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ListesSerresController">
   <children>
      <AnchorPane prefHeight="492.0" prefWidth="624.0">
         <children>
            <TableView fx:id="tableSerres" layoutX="11.0" layoutY="237.0" prefHeight="244.0" prefWidth="599.0" stylesheets="@../application/application.css">
              <columns>
                <TableColumn fx:id="serre_id" prefWidth="77.0" text="id_serre" />
